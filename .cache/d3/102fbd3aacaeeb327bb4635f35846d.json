which suggests default styling will be removed */\r\nul[role=\"list\"],\r\nol[role=\"list\"] {\r\n  list-style: none;\r\n}\r\n\r\n/* Set core root defaults */\r\nhtml {\r\n  scroll-behavior: smooth;\r\n}\r\nhtml:focus-within {\r\n  scroll-behavior: smooth;\r\n}\r\n\r\n/* Set core body defaults */\r\nbody {\r\n  min-height: 100vh;\r\n  text-rendering: optimizeSpeed;\r\n  line-height: 1.5;\r\n}\r\n\r\n/* A elements that don't have a class get default styles */\r\na:not([class]) {\r\n  text-decoration-skip-ink: auto;\r\n}\r\n\r\n/* Make images easier to work with */\r\nimg,\r\npicture {\r\n  max-width: 100%;\r\n  display: block;\r\n}\r\n\r\n/* Inherit fonts for inputs and buttons */\r\ninput,\r\nbutton,\r\ntextarea,\r\nselect {\r\n  font: inherit;\r\n}\r\n\r\n/* Remove all animations, transitions and smooth scroll for people that prefer not to see them */\r\n@media (prefers-reduced-motion: reduce) {\r\n  html:focus-within {\r\n    scroll-behavior: auto;\r\n  }\r\n\r\n  *,\r\n  *::before,\r\n  *::after {\r\n    animation-duration: 0.01ms !important;\r\n    animation-iteration-count: 1 !important;\r\n    transition-duration: 0.01ms !important;\r\n    scroll-behavior: auto !important;\r\n  }\r\n}\r\n","scss\\layout\\_home.scss":"@use 'sass:map';\r\n@use '../abstracts/' as *;\r\n\r\n.home {\r\n  padding: 2em 50px;\r\n\r\n  &__container {\r\n    width: 100%;\r\n    max-width: 1000px;\r\n    min-height: 530px;\r\n    background: map-get($color, secondary);\r\n    border-radius: 10px;\r\n    margin: 0 auto;\r\n    padding: 4em;\r\n    overflow: hidden;\r\n    color: map-get($color, primary);\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr;\r\n    align-items: center;\r\n    justify-content: center;\r\n    margin: 0 auto;\r\n\r\n    .home__left {\r\n      display: flex;\r\n      flex-direction: column;\r\n      padding: 0 3em;\r\n\r\n      h1 {\r\n        font-weight: 400;\r\n        font-size: map-get($fontSize, xl);\r\n        margin-bottom: 15px;\r\n      }\r\n\r\n      p {\r\n        font-size: map-get($fontSize, md);\r\n      }\r\n    }\r\n\r\n    .home__right {\r\n      display: flex;\r\n      justify-content: center;\r\n    }\r\n  }\r\n}\r\n","scss\\components\\_dropdown.scss":"@use '../abstracts/' as *;\r\n\r\n.dropdown {\r\n  position: relative;\r\n  display: inline-block;\r\n  margin-left: 50px;\r\n\r\n  .dropbtn {\r\n    background-color: map-get($color, tertiary);\r\n    color: map-get($color, primary);\r\n    width: 2em;\r\n    height: 2em;\r\n    border-radius: 50px;\r\n    font-size: map-get($fontSize, md);\r\n    border: none;\r\n    cursor: pointer;\r\n    transition: all 0.3s ease-in;\r\n\r\n    @include flex;\r\n  }\r\n\r\n  .dropdown-content {\r\n    visibility: hidden;\r\n    opacity: 0;\r\n    position: absolute;\r\n    right: 0;\r\n    background-color: #f9f9f9;\r\n    min-width: 160px;\r\n    box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);\r\n    z-index: -999;\r\n    transition: all 0.2s ease-in;\r\n\r\n    a {\r\n      color: black;\r\n      padding: 12px 16px;\r\n      font-size: map-get($fontSize, xs);\r\n      text-decoration: none;\r\n      display: block;\r\n\r\n      &:hover {\r\n        background-color: #f1f1f1;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.dropdown:hover .dropdown-content,\r\n.dropdown:focus-within .dropdown-content {\r\n  visibility: visible;\r\n  opacity: 1;\r\n  z-index: 999;\r\n}\r\n\r\n.dropdown:hover .dropbtn {\r\n  background-color: map-get($color, other1);\r\n}\r\n\r\n.mobile__dropdown {\r\n  display: none;\r\n}\r\n","scss\\abstracts\\_mixins.scss":"@use 'sass:map';\r\n@use 'breakpoints' as *;\r\n\r\n// Display Flex align, justify\r\n@mixin flex {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n@mixin mq($key) {\r\n  $size: map-get($breakpoints, $key);\r\n\r\n  @media (max-width: $size) {\r\n    @content;\r\n  }\r\n}\r\n","scss\\layout\\_navbar.scss":"@use 'sass:map';\r\n@use '../abstracts/' as *;\r\n@use '../components/dropdown';\r\n\r\n// Navbar style\r\n.navbar {\r\n  @include flex;\r\n  height: 80px;\r\n  position: -webkit-sticky;\r\n  position: sticky;\r\n  top: 0;\r\n  z-index: 999;\r\n  background: map-get($color, primary);\r\n\r\n  &__container {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    width: 100%;\r\n    max-width: 1100px;\r\n    z-index: 1;\r\n    margin: 0 auto;\r\n    padding: 0 50px;\r\n  }\r\n}\r\n\r\n#navbar-logo {\r\n  display: flex;\r\n  align-items: center;\r\n  text-decoration: none;\r\n  color: map-get($color, text);\r\n  font-weight: 700;\r\n}\r\n\r\n.navbar__menu {\r\n  display: flex;\r\n  align-items: center;\r\n  list-style: none;\r\n  margin: 0;\r\n  padding: 0;\r\n\r\n  .navbar__item {\r\n    height: 80px;\r\n\r\n    .navbar__links {\r\n      @include flex;\r\n      text-decoration: none;\r\n      font-size: map-get($fontSize, sm);\r\n      color: map-get($color, text);\r\n      width: 125px;\r\n      height: 100%;\r\n    }\r\n  }\r\n}\r\n\r\n.navbar__btn {\r\n  @include flex;\r\n}\r\n\r\n@include mq(medium) {\r\n  .mobile__dropdown {\r\n    display: block;\r\n  }\r\n  .navbar__menu {\r\n    display: grid;\r\n    grid-template-columns: auto;\r\n    position: absolute;\r\n    top: -1000px;\r\n    left: 0;\r\n    opacity: 0;\r\n    transition: all 0.5s ease;\r\n    width: 100%;\r\n    padding: 0 2em 2em;\r\n\r\n    &.active {\r\n      background: map-get($color, primary);\r\n      top: 100%;\r\n      opacity: 1;\r\n      z-index: 99;\r\n      height: 50vh;\r\n      transition: all 0.5 ease;\r\n    }\r\n\r\n    .navbar__item {\r\n      width: 100%;\r\n\r\n      .navbar__links {\r\n        text-align: center;\r\n        padding: 2rem;\r\n        width: 100%;\r\n      }\r\n    }\r\n  }\r\n\r\n  .navbar__btn {\r\n    display: none;\r\n  }\r\n\r\n  .navbar__toggle {\r\n    display: flex;\r\n\r\n    .burger {\r\n      align-self: center;\r\n      cursor: pointer;\r\n      padding-left: 20px;\r\n    }\r\n\r\n    .bar {\r\n      width: 24px;\r\n      height: 3px;\r\n      margin: 4px auto;\r\n      border-radius: 50px;\r\n      transition: all 0.2s ease-in-out;\r\n      background: map-get($color, text);\r\n      display: block;\r\n    }\r\n  }\r\n\r\n  #mobile-menu .bar:nth-child(2) {\r\n    width: 19px;\r\n  }\r\n\r\n  #mobile-menu.is-active .bar:nth-child(2) {\r\n    opacity: 0;\r\n  }\r\n\r\n  #mobile-menu.is-active .bar:nth-child(1) {\r\n    transform: translateY(7px) rotate(45deg);\r\n  }\r\n\r\n  #mobile-menu.is-active .bar:nth-child(3) {\r\n    transform: translateY(-7px) rotate(-45deg);\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"33a7e92771773fc724a5e8fbc046cdf7","cacheData":{"env":{}}}